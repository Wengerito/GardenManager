@model IEnumerable<GardenManager.Models.t_plante>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create",null,new { @class = "btn btn-primary" })
</p>
<table class="table">
    <tr>
        <th>
            Nom
        </th>
        <th>
            Type
        </th>
        <th>
            Protocole
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.plaName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.plaType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.plaProtocol)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.plaId }) |
            @Html.ActionLink("Details", "Details", new { id=item.plaId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.plaId })
        </td>
    </tr>
}

</table>
